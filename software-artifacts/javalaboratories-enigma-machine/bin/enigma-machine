#!/bin/bash
set echo off

if [[ -z "${EM_HOME}" ]]; then
  echo "Environment variable EM_HOME undefined"
  exit 1
else
  HOME=${EM_HOME}
fi

VM_OPTIONS=""
JAVA_OPTS="-Xms192m -Xmx192m"
java ${JAVA_OPTS} -jar ${VM_OPTIONS} ${HOME}/lib/javalaboratories-enigma-machine-1.0-0-SNAPSHOT.jar $1 $2 $3 $4 $5 $6 $7 $8 $9
